isbn = {2122633255},
issn = {00315117},
journal = {Perception and Psychophysics},
number = {2},
pages = {206--219},
pmid = {10089756},
title = {{Effects of talker, rate, and amplitude variation on recognition memory for spoken words}},
volume = {61},
year = {1999}
}
