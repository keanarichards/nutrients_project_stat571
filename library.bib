@article{Confer2010,
abstract = {Evolutionary psychology has emerged over the past 15 years as a major theoretical perspective, generating an increasing volume of empirical studies and assuming a larger presence within psychological science. At the same time, it has generated critiques and remains controversial among some psychologists. Some of the controversy stems from hypotheses that go against traditional psychological theories; some from empirical findings that may have disturbing implications; some from misunderstandings about the logic of evolutionary psychology; and some from reasonable scientific concerns about its underlying framework. This article identifies some of the most common concerns and attempts to elucidate evolutionary psychology's stance pertaining to them. These include issues of testability and falsifiability; the domain specificity versus domain generality of psychological mechanisms; the role of novel environments as they interact with evolved psychological circuits; the role of genes in the conceptual structure of evolutionary psychology; the roles of learning, socialization, and culture in evolutionary psychology; and the practical value of applied evolutionary psychology. The article concludes with a discussion of the limitations of current evolutionary psychology.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Confer, Jaime C. and Easton, Judith A. and Fleischman, Diana S. and Goetz, Cari D. and Lewis, David M.G. and Perilloux, Carin and Buss, David M.},
doi = {10.1037/a0018413},
eprint = {arXiv:1011.1669v3},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Confer et al. - 2010 - Evolutionary Psychology Controversies, Questions, Prospects, and Limitations.pdf:pdf},
isbn = {0003-066X},
issn = {0003066X},
journal = {American Psychologist},
keywords = {adaptation,evolutionary psychology,misconceptions,natural selection,psychological mechanisms},
number = {2},
pages = {110--126},
pmid = {20141266},
title = {{Evolutionary Psychology: Controversies, Questions, Prospects, and Limitations}},
volume = {65},
year = {2010}
}
@article{Pedersen2010,
abstract = {Empathy is reported in the research literature as a necessary factor in counseling and psychotherapy, but psychologists have historically interpreted empathy through an exclusively individual focus. Most of the research on empathy has been predicated on a definition of empathy as occurring when one person vicariously experiences the feelings, perceptions, and thoughts of another. In Western cultures, the study of empathy focuses exclusively on the individual, whereas in traditional non-Western cultures, empathy more typically involves an inclusive perspective focusing on the individual and significant others in the societal context. This article explores the reframing of “empathy,” based on an individualistic perspective, into “inclusive cultural empathy,” based on a more relationship-centered perspective, as an alternative interpretation of the empathic process. Psychologists are both the problem and the solution to this dilemma, and the authors call upon the field to take leadership in applying this “inclusive cultural empathy” model. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
author = {Pedersen, Paul B. and Pope, Mark},
doi = {10.1037/0003-066X.65.8.841},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pedersen, Pope - 2010 - Inclusive cultural empathy for successful global leadership.pdf:pdf},
isbn = {0003-066X},
