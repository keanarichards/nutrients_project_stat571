@article{Lowes2018,
abstract = {I examine how matrilineal relative to patrilineal kinship systems affect gender differences in willingness to compete among individuals from societies in Central Africa. These individuals share a common urban environment today, and they are from nearby areas along the matrilineal belt. Individuals participated in a lab experiment in which they completed a task under piece rate pay, tournament pay, and under a payment scheme of their choice. In the experiment, 80{\%} of men and 60{\%} of women chose tournament pay, a standard measure of willingness to compete. I find no evidence that matrilineal kinship closes the gender gap in competition. Women from both matrilineal and patrilineal societies are much less likely to compete than men. The gender gap cannot be explained by performance, beliefs about performance, or risk preferences. Using physiological data, I find that experiencing greater stress in the tournament round relative to the piece rate round makes women less likely to choose competition and that this can partially explain the gender gap.},
author = {Lowes, Sara},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lowes - 2018 - Matrilineal Kinship and Gender Differences in Competition.pdf:pdf},
number = {April},
title = {{Matrilineal Kinship and Gender Differences in Competition? *}},
url = {https://scholar.harvard.edu/files/slowes/files/lowes{\_}competition.pdf},
year = {2018}
}
@article{Zhou2016,
abstract = {The authors find that experimental studies using online samples (e.g., MTurk) often violate the assumption of random assignment, because participant attrition—quitting a study before completing it and getting paid—is not only prevalent, but also varies systemically across experimental conditions. Using standard social psychology paradigms (e.g., ego-depletion, construal level), they observed attrition rates ranging from 30{\%} to 50{\%} (Study 1). The authors show that failing to attend to attrition rates in online panels has grave consequences. By introducing experimental confounds, unattended attrition misled them to draw mind-boggling yet false conclusions: that recalling a few happy events is considerably more effortful than recalling many happy events, and that imagining applying eyeliner leads to weight loss (Study 2). In addition, attrition rate misled them to draw a logical yet false conclusion: that explaining one's view on gun rights decreases progun sentiment (Study 3). The authors offer a partial remedy (Study 4) and call for minimizing and reporting experimental attrition in studies conducted on the Web.},
author = {Zhou, Haotian and Fishbach, Ayelet},
doi = {10.1037/pspa0000056},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhou, Fishbach - 2016 - The pitfall of experimenting on the Web How unattended selective attrition leads to surprising (yet false) resea.pdf:pdf},
