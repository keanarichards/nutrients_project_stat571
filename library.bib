pages = {1--8},
pmid = {27598835},
publisher = {Nature Publishing Group},
title = {{The evolution of acoustic size exaggeration in terrestrial mammals}},
url = {http://dx.doi.org/10.1038/ncomms12739},
volume = {7},
year = {2016}
}
