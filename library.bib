number = {2},
pages = {212--216},
pmid = {20837931},
title = {{The impact of group membership on cooperation and norm enforcement}},
url = {https://papers.ssrn.com/sol3/papers.cfm?abstract{\_}id=892343 http://emlab.berkeley.edu/users/webfac/dellavigna/e218{\_}f05/huffman.pdf},
