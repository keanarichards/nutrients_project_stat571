isbn = {01933973},
issn = {01933973},
journal = {Journal of Applied Developmental Psychology},
keywords = {Adolescents,Attributions,Beliefs about intelligence,Gender differences,Low-income students,Mathematics,Minority students,Reading,Standardized tests,Stereotype threat},
number = {6},
