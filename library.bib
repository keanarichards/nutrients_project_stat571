issn = {0140525X},
journal = {Behavioral and Brain Sciences},
keywords = {Altruism,Cooperation,Cross-cultural research,Experimental economics,Game theory,Public goods game,Self-interest,Ultimatum game},
number = {6},
pages = {795--815},
pmid = {16372952},
