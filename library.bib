keywords = {C91,D82,J16,M51,M52,competitiveness,experiments,firm employment promotion and compensation behavio,gender,tournament prizes},
number = {June},
title = {{Gender Differences in Competitiveness: The Role of Prizes}},
year = {2014}
}
@article{Joel2017,
abstract = {Understanding the multiplicity of ways in which sex can alter the brain is essential to crafting policies and treatments that are beneficial for all human beings. This is particularly true for the field of neuropsychopharmacology, as many neuropsychiatric disorders exhibit gender bias in the frequency, severity, or response to treatment. The goal of this circumspective is to provide two views on the current state of the art of the relations between sex and the brain, relations that are studied almost exclusively by comparing females and males on specific end points, from gene expression to behavior. We start by suggesting a framework for defining what is being measured and what it means. We suggest that 'sex differences' can be classified on four dimensions: (1) persistent vs transient across the lifespan; (2) context independent vs dependent; (3) dimorphic vs continuous; and (4) a direct vs an indirect consequence of sex. To accurately classify a sex difference along these dimensions, one may need to compare females and males under varied conditions. We next discuss current data on the mechanisms of sexual differentiation of the brain and on sex differences in the brain to conclude that the brain of each male and female is a mosaic of relative masculinization, feminization, and sameness, which theoretically could produce an infinite variety of individuals. We also raise the possibility that sex differences in the brain are canalized, which may act to both enhance and restrain variation between males and females. We end by discussing ways to consider sex when studying neuropsychiatric disorders.},
author = {Joel, Daphna and McCarthy, Margaret M.},
doi = {10.1038/npp.2016.79},
file = {:C$\backslash$:/Users/keana/Downloads/npp201679a.pdf:pdf},
isbn = {1740-634X},
issn = {1740634X},
journal = {Neuropsychopharmacology},
number = {2},
pages = {1--7},
pmid = {27240659},
title = {{Incorporating Sex As a Biological Variable in Neuropsychiatric Research: Where Are We Now and Where Should We Be?}},
volume = {42},
year = {2017}
}
@article{Sashkin1990,
abstract = {Examined the perceptions of store managers, department managers (DMs), and non-supervisory employees (NSEs) from high- and low-cost retail stores regarding the climate of fairness they encountered in organizational life. Nine specific elements of fairness were used: trust, consistency, truthfulness, integrity, expectations, equity, influence, justice, and respect. There were no differences between store managers or DMs from low- and high-cost stores. However, NSEs at low-cost stores viewed the fairness climate in a different way than did NSEs at high-cost stores. NSEs considered trust, truthfulness, integrity, and justice to be important. Findings suggest that when employees reported organizational climates that were deficient in these elements of fairness, the organization bore significantly higher costs in the areas of employee sickness and accident compensation costs. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
