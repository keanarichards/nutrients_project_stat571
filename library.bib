@article{Dovidio2017,
abstract = {Classic research on the contact hypothesis focused on the direct relationship between the antecedents (conditions under which contact occurs) and the outcomes (primarily, the reduction of prejudice) of intergroup contact. Recent work has taken a broader view of contact processes and effects. We review key developments over the past 20 years, identifying different forms of contact, factors that mediate and moderate the effects of contact, and both the nature and temporal stage and the varied outcomes of contact. We then identify several research directions to address pressing theoretical and practical issues. These issues concern (a) group processes and intergroup relations, (b) intergroup contact in the context of multiple categorization, (c) structural- and individual-level processes, (d) a broader range of individual-level outcomes (e.g., health), and (e) impact on social change. Contact theory and research provides a comprehensive conceptual foundation, allied to a range of powerful empirical technique...},
author = {Dovidio, John F. and Love, Angelika and Schellhaas, Fabian M. H. and Hewstone, Miles},
doi = {10.1177/1368430217712052},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dovidio et al. - 2017 - Reducing intergroup bias through intergroup contact Twenty years of progress and future directions.pdf:pdf},
isbn = {1368-4302},
issn = {1368-4302},
journal = {Group Processes {\&} Intergroup Relations},
number = {June},
pages = {136843021771205},
title = {{Reducing intergroup bias through intergroup contact: Twenty years of progress and future directions}},
url = {http://journals.sagepub.com/doi/10.1177/1368430217712052},
year = {2017}
}
