@article{Nesse2005,
abstract = {Many of the body's adaptive responses, such as pain, fever, and fear, are defenses that remain latent until they are aroused by cues that indicate the presence of a threat. Natural selection should shape regulation mechanisms that express defenses only in situations where their benefits exceed their costs, but defenses are often expressed in situations where they seem unnecessary, with much resulting useless suffering. An explanation emerges from a signal detection analysis of the costs and benefits that shaped defense regulation mechanisms. Quantitative modeling of optimal regulation for all-or-none defenses and for continuously variable defenses leads to several conclusions. First, an optimal system for regulating inexpensive all-or-none defenses against the uncertain presence of large threats will express many false alarms. Second, the optimum level of expression for graded defenses is not at the point where the costs of the defense and the danger are equal, but is instead where the marginal cost of additional defense exceeds the marginal benefit. Third, in the face of uncertainty and skewed payoff functions, the optimal response threshold may not be the point with the lowest cost. Finally, repeated exposures to certain kinds of danger may adaptively lower response thresholds, making systems vulnerable to runaway positive feedback. While we await quantitative data that can refine such models, a general theoretical perspective on the evolution of defense regulation can help to guide research and assist clinical decision making. ?? 2005 Elsevier Inc. All rights reserved.},
author = {Nesse, Randolph M.},
doi = {10.1016/j.evolhumbehav.2004.08.002},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nesse - 2005 - Natural selection and the regulation of defenses. A signal detection analysis of the smoke detector principle.pdf:pdf},
isbn = {1090-5138},
issn = {10905138},
journal = {Evolution and Human Behavior},
keywords = {Control theory,Cybernetics,Defenses,Evolution,False alarms,Natural selection,Pharmacology,Signal detection theory,Systems theory},
number = {1},
pages = {88--105},
pmid = {15884737},
title = {{Natural selection and the regulation of defenses. A signal detection analysis of the smoke detector principle}},
volume = {26},
year = {2005}
}
