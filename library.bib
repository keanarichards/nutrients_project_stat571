@article{Hanek2016,
abstract = {In a series of 8 studies, we examine whether gender differences in$\backslash$ncompetition entry preferences are moderated by the size of the$\backslash$ncompetition. Drawing on theories of gender roles and stereotypes, we$\backslash$nshow that women, relative to men, prefer to enter smaller compared with$\backslash$nlarger competitions. Studies 1a and 1b demonstrate this effect in$\backslash$nobservational data on preferences for working in differently sized firms$\backslash$nand applying to differently sized colleges. Studies 2a and 2b replicate$\backslash$nthe effect with real behavioral decisions in different domains. We also$\backslash$nfind empirical evidence that prescriptive gender norms and stereotypes$\backslash$nunderlie this effect. In Study 3, we find experimental evidence that$\backslash$nwomen and men differ in their preferences for differently sized groups$\backslash$nunder competition, but not in noncompetitive settings. Three additional$\backslash$nexperimental studies (Studies 4, 5a, and 5b) show that perceptions of$\backslash$ncomfort in small versus larger competitions underlie women's$\backslash$npreferences. These findings suggest that women's preferences for smaller$\backslash$ncompetitions may be driven by an adherence to prescriptive gender norms.$\backslash$nWe discuss the implications of the current findings for gender$\backslash$ninequalities in organizations.},
