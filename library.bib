issn = {0146-1672},
journal = {Personality and Social Psychology Bulletin},
keywords = {2016,2017,an,discrimination continue to be,incidences of prejudice and,interpersonal confrontation,prejudice reduction,racism,received september 2,revision accepted october 13,stereotypes},
