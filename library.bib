isbn = {1060-1538},
issn = {10601538},
journal = {Evolutionary Anthropology},
number = {2},
pages = {112--118},
pmid = {8590},
title = {{Are human faces and voices ornaments signaling common underlying cues to mate value?}},
volume = {17},
year = {2008}
}
