journal = {Journal of Applied Psychology},
keywords = {Attitudes,Emotion,Interpersonal effects,Persuasion,Social influence},
number = {4},
pages = {1124--1142},
pmid = {25402955},
title = {{The persuasive power of emotions: Effects of emotional expressions on attitude formation and change}},
volume = {100},
year = {2015}
}
