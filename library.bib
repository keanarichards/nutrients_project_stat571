journal = {Evolutionary Anthropology},
keywords = {Exploitation,Hidden preferences,Influence,Learning-based biases,Sensory traps},
number = {4},
pages = {136--145},
pmid = {25116845},
title = {{In the eye (and ears) of the beholder: Receiver psychology and human signal design}},
volume = {23},
year = {2014}
