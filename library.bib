@article{Ekstrom2003,
abstract = {Place cells of the rodent hippocampus constitute one of the most striking$\backslash$nexamples of a correlation between neuronal activity and complex behaviour$\backslash$nin mammals. These cells increase their firing rates when the animal$\backslash$ntraverses specific regions of its surroundings, providing a context-dependent$\backslash$nmap of the environment. Neuroimaging studies implicate the hippocampus$\backslash$nand the parahippocampal region in human navigation. However, these$\backslash$nregions also respond selectively to visual stimuli. It thus remains$\backslash$nunclear whether rodent place coding has a homologue in humans or$\backslash$nwhether human navigation is driven by a different, visually based$\backslash$nneural mechanism. We directly recorded from 317 neurons in the human$\backslash$nmedial temporal and frontal lobes while subjects explored and navigated$\backslash$na virtual town. Here we present evidence for a neural code of human$\backslash$nspatial navigation based on cells that respond at specific spatial$\backslash$nlocations and cells that respond to views of landmarks. The former$\backslash$nare present primarily in the hippocampus, and the latter in the parahippocampal$\backslash$nregion. Cells throughout the frontal and temporal lobes responded$\backslash$nto the subjects' navigational goals and to conjunctions of place,$\backslash$ngoal and view.},
archivePrefix = {arXiv},
