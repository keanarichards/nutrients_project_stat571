journal = {Journal of Neuroscience},
number = {6},
pages = {2013--2017},
pmid = {22323715},
title = {{No Dissociation between Perception and Action in Patient DF When Haptic Feedback is Withdrawn}},
url = {http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.3413-11.2012},
volume = {32},
year = {2012}
}
