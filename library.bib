isbn = {0930-2794},
issn = {14322218},
journal = {Surgical Endoscopy and Other Interventional Techniques},
keywords = {360-degree survey,Emotional intelligence,Leadership,Leadership development,PULSE 360,PULSE survey,Physician feedback,Physician-leaders,Professional medical association,Self-other agreement},
