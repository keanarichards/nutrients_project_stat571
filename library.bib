issn = {03616843},
journal = {Psychology of Women Quarterly},
keywords = {equity (payment),human sex differences,impression management,interpersonal communication,negotiation,salaries,stereotyped attitudes},
number = {1},
pages = {80--96},
title = {{How can women escape the compensation negotiation dilemma? Relational accounts are one answer}},
volume = {37},
year = {2013}
}
