journal = {Early Childhood Research Quarterly},
keywords = {Attitude change,Early childhood,Ethnic racial bias,Media intervention,Prejudice reduction,Program evaluation},
pages = {110--122},
title = {{Evaluation of an intervention using cross-race friend storybooks to reduce prejudice among majority race young children}},
volume = {40},
year = {2017}
}
