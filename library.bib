file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shapiro, Neuberg - 2007 - From stereotype threat to stereotype threats Implications of a multi-threat framework for causes, moderators,.pdf:pdf},
isbn = {1088-8683$\backslash$n1532-7957},
issn = {10888683},
journal = {Personality and Social Psychology Review},
keywords = {Stereotype threat,Stereotyped attitudes,Stereotyped behaviors,Stigma,Threat},
number = {2},
pages = {107--130},
