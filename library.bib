@article{Goette2006,
abstract = {Due to incomplete contracts, efficiency of an organization depends on willingness of$\backslash$nindividuals to take non-selfish actions, e.g., cooperate when there is no incentive to do so, or$\backslash$npunish inefficient actions by others. Organizations also constitute a social boundary, or$\backslash$ngroup. We investigate whether this social aspect of organizations has an important benefit,$\backslash$nfostering unselfish cooperation and norm enforcement within the group, but whether there is$\backslash$nalso a dark side, in the form of hostility between groups. Our experiment provides the first$\backslash$nevidence without the confounding effect of self-selection into groups. Individuals are$\backslash$nrandomly assigned to different platoons during a four-week portion of officer training in the$\backslash$nSwiss Army. We conduct choice experiments – simultaneous prisoner's dilemma games, with$\backslash$nand without third-party punishment – in week three. Random assignment significantly$\backslash$nincreases willingness to cooperate with fellow platoon members. Assignment does not lead to$\backslash$nhostility, in the sense of vindictive punishment of outsiders, but does affect norm$\backslash$nenforcement, enhancing willingness to enforce a norm of cooperation towards fellow platoon$\backslash$nmembers. This suggests that the social aspect of organizations motivates efficient behavior$\backslash$neven when ordinary incentives fail, and helps explain practices designed to foster social ties$\backslash$nor group identification within an organization.},
author = {Goette, Lorenz and Huffman, David and Meier, Stephan},
doi = {10.1257/000282806777211658},
file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Goette, Huffman, Meier - 2006 - The impact of group membership on cooperation and norm enforcement.pdf:pdf},
