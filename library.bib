issn = {0019-7858},
journal = {Industrial and Commercial Training},
keywords = {360 degree assessment,leadership,management,paper type research paper,power and influence,team roles},
number = {7},
pages = {397--405},
title = {{360 Degree Assessments of Team Role Behaviours in Different Contexts}},
