file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pickett et al. - 2012 - Reconsidering the relationship between perceived neighborhood racial composition and whites' perceptions of vict.pdf:pdf},
