file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Seibert, Wang, Courtright - 2011 - Antecedents and Consequences of Psychological and Team Empowerment in Organizations A Meta-Analytic R.pdf:pdf},
isbn = {0021-9010},
