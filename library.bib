journal = {Motivation Science},
keywords = {activity,aging,beneficial to cognitive health,cifically,cognition,cognitive engagement has been,have been,high levels of engagement,in later life,motivation,selective engagement,shown to be,spe-},
number = {1},
pages = {26--38},
title = {{Linkages Between Resources, Motivation, and Engagement in Everyday Activities.}},
