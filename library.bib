isbn = {1-4338-0997-4 (Hardcover), 978-1-4338-0997-2 (Hardcover), 978-1-4338-0996-5 (Hardcover) (Set)},
journal = {APA educational psychology handbook, Vol 1: Theories, constructs, and critical issues.},
