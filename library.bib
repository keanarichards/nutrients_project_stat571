file = {:C$\backslash$:/Users/keana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hayes et al. - 2003 - Neural plasticity following auditory training in children with learning problems.pdf:pdf},
isbn = {13882457},
issn = {13882457},
journal = {Clinical Neurophysiology},
keywords = {Auditory physiology,Auditory processing,Neural plasticity,Reading disability,Speech sound perception and speech perception in n},
number = {4},
pages = {673--684},
pmid = {12686276},
title = {{Neural plasticity following auditory training in children with learning problems}},
volume = {114},
