journal = {Organizational Behavior and Human Decision Processes},
pages = {28--44},
publisher = {Elsevier Inc.},
title = {{What “blindness” to gender differences helps women see and do: Implications for confidence, agency, and action in male-dominated environments}},
url = {http://dx.doi.org/10.1016/j.obhdp.2017.07.004},
volume = {142},
year = {2017}
}
@article{Murphy2007,
